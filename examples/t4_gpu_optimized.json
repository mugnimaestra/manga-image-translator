{
  "detector": {
    "detector": "default",
    "detection_size": 1536,
    "text_threshold": 0.5,
    "box_threshold": 0.7,
    "unclip_ratio": 2.3
  },
  "ocr": {
    "ocr": "48px",
    "min_text_length": 0
  },
  "inpainter": {
    "inpainter": "lama_large",
    "inpainting_size": 1024,
    "inpainting_precision": "fp16"
  },
  "translator": {
    "translator": "sugoi",
    "target_lang": "IND"
  },
  "render": {
    "renderer": "manga2eng",
    "alignment": "auto",
    "direction": "horizontal",
    "font_size_offset": -4,
    "font_size_minimum": 10,
    "no_hyphenation": true
  },
  "upscale": {
    "upscaler": "esrgan",
    "upscale_ratio": null,
    "revert_upscaling": false
  },
  "colorizer": {
    "colorizer": "none"
  },
  "mask_dilation_offset": 20,
  "kernel_size": 3
}
